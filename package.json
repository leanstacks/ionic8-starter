{
  "name": "ionic8-starter",
  "description": "A template to kickstart Ionic + React applications utilizing an opinionated technology stack for optimal testability, maintainability, and operability.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leanstacks/ionic8-starter.git"
  },
  "license": "MIT",
  "version": "1.1.0-alpha.1",
  "type": "module",
  "scripts": {
    "android": "npx cap sync android && npx cap open android",
    "prebuild": "./.appflow/scripts/prebuild.sh",
    "build": "tsc && vite build",
    "clean": "rimraf coverage dist",
    "dev": "vite --open",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "lint": "eslint . --ext .ts --max-warnings 0",
    "lint:fix": "eslint . --ext .ts --fix",
    "prepare": "husky",
    "preview": "vite preview",
    "test": "vitest",
    "test:ci": "vitest run --coverage --silent",
    "test:coverage": "vitest --coverage",
    "test:e2e": "cypress run"
  },
  "dependencies": {
    "@capacitor/android": "7.2.0",
    "@capacitor/app": "7.0.1",
    "@capacitor/assets": "3.0.5",
    "@capacitor/core": "7.2.0",
    "@capacitor/haptics": "7.0.1",
    "@capacitor/keyboard": "7.0.1",
    "@capacitor/status-bar": "7.0.1",
    "@fortawesome/fontawesome-svg-core": "7.2.0",
    "@fortawesome/free-solid-svg-icons": "7.2.0",
    "@fortawesome/react-fontawesome": "3.2.0",
    "@ionic/react": "8.7.17",
    "@ionic/react-router": "8.7.17",
    "@tanstack/react-query": "5.90.21",
    "@tanstack/react-query-devtools": "5.91.3",
    "axios": "1.13.5",
    "classnames": "2.5.1",
    "dayjs": "1.11.19",
    "formik": "2.4.9",
    "i18next": "25.0.1",
    "i18next-browser-languagedetector": "8.0.5",
    "lodash": "4.17.23",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-error-boundary": "5.0.0",
    "react-i18next": "15.4.1",
    "react-router": "5.3.4",
    "react-router-dom": "5.3.4",
    "uuid": "13.0.0",
    "yup": "1.7.1"
  },
  "devDependencies": {
    "@capacitor/cli": "7.2.0",
    "@eslint/js": "9.39.2",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/eslint__js": "8.42.3",
    "@types/lodash": "4.17.24",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@types/react-router": "5.1.20",
    "@types/react-router-dom": "5.3.3",
    "@types/uuid": "10.0.0",
    "@vitejs/plugin-legacy": "6.1.0",
    "@vitejs/plugin-react": "4.4.1",
    "@vitest/coverage-v8": "3.1.2",
    "cypress": "15.10.0",
    "eslint": "9.39.2",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.5.1",
    "globals": "16.0.0",
    "husky": "^9.1.7",
    "jsdom": "26.1.0",
    "msw": "2.12.10",
    "prettier": "3.8.1",
    "rimraf": "6.1.3",
    "sass": "1.86.3",
    "terser": "5.39.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.55.0",
    "vite": "6.3.2",
    "vitest": "3.1.2"
  }
}
